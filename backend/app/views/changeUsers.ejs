<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">

<table>
    <thead class="thead-dark">
    <tr>
        <th>Name</th>
        <th>Surname</th>
        <th>Email</th>
    </tr>
    </thead>
    <% users.forEach( element => { %>
        <tr>
            <td><%= element.name %></td>
            <td><%= element.surname %></td>
            <td><%= element.email %></td>
            <td><a href="/api/v1/players/changeUsers/<%= element.email %>/user">Change to User</a></td>
            <td><a href="/api/v1/players/changeUsers/<%= element.email %>/admin">Change to Admin</a></td>
        </tr>
    <% }) %>
</table>
<br>
<input type="button", value="Back", onclick="window.location.href='/api/v1/players/general'">